{
  "name": "service-discovery",
  "version": "1.0.3",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/index.ts",
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,md}'"
  },
  "dependencies": {
    "app-life-cycle-pkg": "git+git@github.com:spalx/app-life-cycle-pkg.git#1.0.7",
    "common-loggers-pkg": "git+git@github.com:spalx/common-loggers-pkg.git#1.0.4",
    "http-transport-adapter": "git+git@github.com:spalx/http-transport-adapter.git#1.0.7",
    "ioredis": "^5.8.1",
    "rest-pkg": "git+git@github.com:spalx/rest-pkg.git#1.0.4",
    "service-discovery-pkg": "git+git@github.com:spalx/service-discovery-pkg.git#1.0.3",
    "transport-pkg": "git+git@github.com:spalx/transport-pkg.git#1.1.8"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0"
  }
}
